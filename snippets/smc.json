{
    "Sysmon Comment": {
        "prefix": "comment",
        "body": [ 
            "<!--$1-->"
        ],
        "description": "Initial base configuration for Sysmon"
    },

    "Sysmon condition": {
        "prefix": "condition",
        "body": [
            "condition=\"${1|is,is not,contains,contains any,contains all,excludes,begin with,end with,less than,more than,image|}\""
        ],
        "description": "Sysmon event field Company filter."
    },

    "Template Sysmon Config": {
        "prefix": "sysmon_config",
        "body": [ 
            "<Sysmon schemaversion=\"4.22\">",
            "  <!-- Hashing algorithms that can be used are md5,sha1,sha256,imphash or * for all,", 
            "  more than once can be specified separated by using comas -->",
            "  <HashAlgorithms>${1|md5,sha1,sha256,imphash,*|}</HashAlgorithms>",
            "  <!-- Checking for signature revocation for drivers. -->",
            "  <CheckRevocation/>",
            "  <EventFiltering>",
            "    ${2}          ",
            "  </EventFiltering>",
           "</Sysmon>"
        ],
        "description": "Initial base configuration for Sysmon"
    },

    "Sysmon RuleGroup": {
        "prefix": "rulegroup",
        "body": [ 
            "<RuleGroup name=\"$1\" groupRelation=\"${2|and,or|}\">",
              "${3}",
            "</RuleGroup>"
        ],
        "description": "Sysmon Rule Group"
    },

    "Sysmon Rule": {
